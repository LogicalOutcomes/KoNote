.manageProgramClientsDialog
	width 600px
	max-height 400px
	display flex
	overflow auto
	
	> div
		flex 1
		
		&:first-child
			margin-right 10px
			overflow auto
		&:last-child
			margin-left 10px
			overflow auto
			
		> .panel-heading .badge
			float right
			
		> .panel-body
			
			&.noData
				text-align center
				padding 15px
				font-style italic
				
	.programClients
		h3
			border-bottom 2px solid #eee
			padding-bottom 10px

		
	table > thead
		font-weight bold
		
	table td > .noId
		text-align center
		