.dropdown-menu
    max-height 250px
    overflow-y auto
.btn-toolbar
	float right

.createClientFileDialog
	min-width 580px

	.panelContainer
		display flex

		.panel-left
			padding-right 20px
			width 280px

		.panel-right
			width 300px

	.birthdateLabel
		vertical-align middle

	.btnReset
		margin 0px 0px 5px 5px
		padding 0px
		outline none
		text-decoration none
		&:focus
			outline none

	.dropdown
		margin 0px 15px 15px 0px

	#programsContainer
		width 100%
		max-height 280px
		overflow-y auto
		overflow-x hidden

		.programOptionButton
			margin-right 10px
			margin-bottom 10px
			outline none

			line-height 1.5em
			vertical-align middle

			&:last-child
				margin-right 0

			.colorKeyBubble
				margin-right 10px

				> .bubbleContents
					font-size 1.5em
