.dropdown-menu
    max-height 250px
    overflow-y auto

.createClientFileDialog
	min-width 300px

	#programsContainer
		width 350px
		max-height 150px
		overflow-y auto
		overflow-x hidden

		.programOptionButton
			margin-right 10px
			margin-bottom 10px
			outline none

			line-height 1.5em
			vertical-align middle

			&:last-child
				margin-right 0

			.colorKeyBubble
				margin-right 10px

				> .bubbleContents
					font-size 1.5em
