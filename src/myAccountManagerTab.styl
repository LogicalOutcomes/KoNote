#myAccountManagerTab
	.main
		display flex

		h3
			font-weight 600
			border-bottom 1px solid #eee
			padding-bottom 15px

		& > div
			flex 1
			padding 0px 25px 25px 25px

		.form-group
			position relative
			margin-top 25px

			.glyphicon
				transition all 0.15s ease
				opacity 0
			&.has-success .glyphicon
				opacity 1