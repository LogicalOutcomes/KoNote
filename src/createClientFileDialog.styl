.createClientFileDialog
	min-width 580px
	max-width 600px

	#programsContainer
		width 100%
		max-height 280px
		overflow-y auto
		overflow-x hidden

		.programOptionButton
			margin-right 10px
			margin-bottom 10px
			outline none

			line-height 1.5em
			vertical-align middle

			&:last-child
				margin-right 0

			.colorKeyBubble
				margin-right 10px

				> .bubbleContents
					font-size 1.5em


	&.singlePanel
		min-width 280px
		max-width 365px

		#programsContainer
			max-height 89px



	.btn-toolbar
		float right

	.panelContainer
		display flex

		&.singlePanel
			min-width 280px

		.panel-left
			flex 1

		.panel-right
			padding-left 20px
			flex 1

			.template-form-group
				.dropdown .dropdown-menu
					max-height 162px
					overflow-y auto

	.birthdateLabel
		vertical-align middle

	.btnReset
		margin 0px 0px 5px 5px
		padding 0px
		outline none
		text-decoration none
		&:focus
			outline none

	.dropdown
		margin 0px 15px 15px 0px


